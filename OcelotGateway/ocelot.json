{
  "Routes": [
    {
      "DownstreamPathTemplate": "/login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/register",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/auth/register",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/logout",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/auth/logout",
      "UpstreamHttpMethod": [ "POST" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },

    {
      "DownstreamPathTemplate": "/shorten",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "urlservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/shorten",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/{shortCode}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "urlservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/{shortCode}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/my-urls",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "urlservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/url/my-urls",
      "UpstreamHttpMethod": [ "GET" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/delete/{shortCode}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "urlservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/url/delete/{shortCode}",
      "UpstreamHttpMethod": [ "DELETE" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/update/{shortCode}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "urlservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/url/update/{shortCode}",
      "UpstreamHttpMethod": [ "PUT" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },

    {
      "DownstreamPathTemplate": "/generate-qr",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "qrservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/qr/generate-qr",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/qr/{shortCode}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "qrservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/qr/{shortCode}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8000"
  }
}